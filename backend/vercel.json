{
    "installCommand" : "npm install --legacy-peer-deps",
    "version": 2,
    "builds":[
        {
            "src": "server.js",
            "use": "@vercel/node"
        },{
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)", 
            "dest": "server.js", 
            "methods": ["FECTH","GET", "POST", "PUT", "DELETE"], 
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/auth/(.*)", 
            "dest": "server.js", 
            "methods": ["FECTH","GET", "POST", "PUT", "DELETE"], 
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/user/(.*)", 
            "dest": "server.js", 
            "methods": ["FECTH","GET", "POST", "PUT", "DELETE"], 
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}